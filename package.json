  "scripts": {
    "build": "cross-env NODE_ENV=development TARGET=local webpack --config ./conf/webpack.build.conf.babel.js",
    "production": "cross-env NODE_ENV=production TARGET=remote webpack --config ./conf/webpack.production.conf.babel.js",
    "devserver": "cross-env NODE_ENV=development TARGET=local webpack serve --config ./conf/webpack.devserver.conf.babel.js",
    "devwatch": "cross-env NODE_ENV=development TARGET=remote webpack --config ./conf/webpack.devwatch.conf.babel.js",
    "bundle-report": "webpack-bundle-analyzer ./dist/stats.json",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "webserver": "webserver 888 ./dist"
  },
